<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="VectSharp.DemoAnimation.MainWindow"
        Title="VectSharp.DemoAnimation">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="1*"/>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="Auto"/>
			<ColumnDefinition Width="1*"/>
		</Grid.ColumnDefinitions>
		<Viewbox Name="mainViewBox" Grid.ColumnSpan="2" Margin="5">

		</Viewbox>
		<Button Name="playPauseButton" Width="100" Grid.Row="1" Margin="5" HorizontalContentAlignment="Center"></Button>
		<Slider Grid.Column="1" Grid.Row="1" Name="progressSlider" Margin="5"/>
	</Grid>
	
</Window>
